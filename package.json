{
    "name": "fanuc-karel-syntax",
    "displayName": "Fanuc-Karel-Syntax",
    "description": "Fanuc Karel (.kl) Syntax Support",
    "version": "0.0.2",
    "publisher": "fabi",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fanuc-karel",
            "aliases": ["Fanuc-Karel(.kl)", "fanuc-karel"],
            "extensions": [".kl", ".klh", ".klt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fanuc-karel",
            "scopeName": "source.kl",
            "path": "./syntaxes/fanuc-karel.tmLanguage.json"
        }],
        "snippets":[{
                "language": "fanuc-karel",
                "path": "./snippets/fanuc-karel.json"
        }],
        "themes": [
            {
                "label": "Karel_Theme",
                "id": "karel",
                "uiTheme": "vs-dark",
                "path": "./themes/karel_theme-color-theme.json"
            }
        ]
    }
}
