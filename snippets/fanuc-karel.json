{
    "For_Loop": {
        "prefix": "FOR",
        "body": [
          "FOR ${1:element} TO ${2:element} DO",
          "\t$0",
          "ENDFOR"
        ],
        "description": "For Loop"
    },

    "IF statement": {
        "prefix": "IF",
        "body": [
          "IF ${1:condition} THEN",
          "\t$0",
          "ENDIF"
        ],
        "description": "IF Statement"
    },

    "IF ELSE statement": {
        "prefix": "IFELSE",
        "body": [
          "IF ${1:condition} THEN",
          "\t$2",
          "ELSE",
          "\t$3",
          "ENDIF"
        ],
        "description": "IF ELSE Statement"
    },

    "Select statement": {
        "prefix": "SELECT",
        "body": [
          "SELECT ${1:element} OF",
          "CASE(${2:element}):",
          "\t$3",
          "ELSE:",
          "\t$4",
          "ENDSELECT"
        ],
        "description": "SELECT Statement"
    },

    "While loop": {
        "prefix": "WHILE",
        "body": [
          "WHILE ${1:element} DO",
          "\t$2",
          "ENDWHILE"
        ],
        "description": "While loop"
    },

    "Repeat loop": {
        "prefix": "REPEAT",
        "body": [
          "REPEAT",
          "\t$0",
          "UNTIL(${1:element})"
        ],
        "description": "Repeat loop"
    }
}
